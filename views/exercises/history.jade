extends layout

block thirdTab
	if history
		table.table.table-striped.table-condensed
			thead
				tr
					th Revision
					th Created
					th User
			tbody
				each exercise, index in history.reverse()
					tr
						td= history.length - index
						td= exercise.createdAt.toISOString()
						td User
	else
		.alert.alert-info.
			There are no previous versions of this exercise.
